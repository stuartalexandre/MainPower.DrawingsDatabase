<Window x:Class="MainPower.DrawingsDatabase.Gui.GuiSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GUISettings" Height="371" Width="393" ResizeMode="CanResizeWithGrip" ShowInTaskbar="False" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner" Loaded="Window_Loaded">
    <Grid>
        <DockPanel Margin="0,0,0,41" LastChildFill="True">
            <Expander  DockPanel.Dock="Top" Header="Substations" IsExpanded="True" >
                <Grid>
                    <TextBox Margin="0,0,0,0" Name="txtSubs" AcceptsReturn="True" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Auto" />
                </Grid>
            </Expander>

        </DockPanel>
        <Button Content="Save and Close" Margin="0,0,12,12" Name="btnSave" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="98" Click="btnSave_Click" />

    </Grid>
</Window>
