        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Drawing number in database checkerer" Height="368"
        Width="382">
  <Grid>
    <TextBox Margin="12,46,0,12" Name="textBox1" HorizontalAlignment="Left" Width="154" AcceptsReturn="True" />
    <Label Content="Paste list of numbers here" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0"
           Name="label1" VerticalAlignment="Top" />
    <ListBox Margin="172,46,12,12" Name="listBox1" />
    <Button Content="Go" Height="23" HorizontalAlignment="Left" Margin="172,13,0,0" Name="button1"
            VerticalAlignment="Top" Width="176" Click="button1_Click" />
  </Grid>
</Window>